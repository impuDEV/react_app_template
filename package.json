{
	"name": "react_app_template",
	"version": "0.1.0",
	"description": "react preconfigured template for a new project developing fast starting",
	"main": "index.js",
	"scripts": {
		"start": "webpack serve --env port=3000",
		"build:prod": "webpack --env mode=production",
		"build:dev": "webpack --env mode=development",
		"prettier": "npx prettier --write **/*.{ts,tsx,json}",
		"lint:ts": "eslint \"**/*.{ts,tsx}\"",
		"lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
		"lint:scss": "npx stylelint \"**/*.scss\"",
		"lint:scss:fix": "npx stylelint \"**/*.scss\" --fix"
	},
	"lint-staged": {
		"**/*.{ts,tsx}": [
			"prettier --list-different",
			"eslint"
		]
	},
	"author": "impuDEV<impudev@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
		"@svgr/webpack": "^8.1.0",
		"@types/circular-dependency-plugin": "^5.0.8",
		"@types/node": "^17.0.21",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.0",
		"@types/react-router-dom": "^5.3.3",
		"@types/webpack": "^5.28.5",
		"@types/webpack-bundle-analyzer": "^4.4.1",
		"@types/webpack-dev-server": "^4.7.1",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"circular-dependency-plugin": "^5.2.2",
		"copy-webpack-plugin": "^10.2.4",
		"css-loader": "^6.6.0",
		"eslint": "^8.10.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-i18next": "^6.1.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-impudev-fsd": "^0.0.2",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.29.2",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-unused-imports": "^4.1.4",
